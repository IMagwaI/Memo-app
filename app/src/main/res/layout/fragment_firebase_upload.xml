<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="56sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10sp"
        app:srcCompat="@mipmap/google" />

    <ImageButton
        android:id="@+id/upload"
        android:layout_width="78sp"
        android:layout_height="69sp"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80sp"
        android:background="@mipmap/upload"
        android:text="upload" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="267sp"
        android:layout_height="47sp"
        android:layout_below="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="136sp"
        android:text="Upload your saved data "
        android:textSize="24sp" />
</RelativeLayout>